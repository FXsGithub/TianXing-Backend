<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tongji.enso.mybatisdemo.mapper.online.MeteoMapper">

    <select id="findAll" resultType="Meteo">
        select * from meteo
    </select>

    <select id="findById" resultType="Meteo">
        select * from meteo where id=#{id}
    </select>

</mapper>

